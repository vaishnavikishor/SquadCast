# Function to replace Unicode with actual emojis
def unicode_to_emoji(text):
    import re
    def replace_unicode(match):
        return chr(int(match.group(1), 16))
    
    return re.sub(r'U\+([0-9A-Fa-f]{4})', replace_unicode, text)

# Convert and print the result
final_text = unicode_to_emoji(decrypted_text)
print(final_text)
